class PrefixNameSearch
  source_url: '/things/name_search'

  constructor: ->

  search: (prefix, callback) ->
    matches = $.ajax
      url: @source_url
      dataType: 'json'
      data:
        query: prefix
    .success (data) =>
      callback(data)
    .error () =>
      callback([])


window.PrefixNameSearch = new PrefixNameSearch